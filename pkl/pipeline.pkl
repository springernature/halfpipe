amends "halfpipe.pkl"

team = "my-team"
pipeline = "my-pipe"
platform = "actions"

tasks {
  new Run {
    name = "test"
    timeout = 12.min
    script = "./test.sh"
  }
  new DockerCompose {
    command = "./build"
  }
  new Parallel {
    tasks {
      new Sequence{
        tasks {
          new DockerCompose {}
          new DockerCompose {}
        }
      }
      new Sequence{
        tasks {
          new DockerCompose {}
          new DockerCompose {}
        }
      }
    }
  }
}
